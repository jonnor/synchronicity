
INPORT=led0.IN:LED0
OUTPORT=led0.OUT:LED0
OUTPORT=sensor0.DISTANCE:DISTANCE_NORTH
OUTPORT=sensor0.TRIGGERED:ALIVE

# Distance sensors 
timer(Timer) OUT -> TRIGGER sensor0(UltrasoundSR04)
sensor0 DISTANCE -> IN sensor0f2(Forward)
sensor0 TRIGGERED -> IN sensor0f(Forward)

# LEDs
led0(DigitalWrite) OUT -> IN led0f(Forward)

# Config
'3' -> PIN led0
'200' -> INTERVAL timer # needs to be at least N sensors (4) * max time (~30ms)
'7' -> TRIGPIN sensor0, '8' -> ECHOPIN sensor0

# Debugging
debugTimer(Timer) OUT -> IN debugToggle(ToggleBoolean) OUT -> IN debugLed(DigitalWrite) OUT -> IN debugF(Forward)
'13' -> PIN debugLed
'1000' -> INTERVAL debugTimer
